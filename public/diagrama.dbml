table ajustes {
  id int [pk, increment]
  nombre varchar
  descripcion text
  sucursal varchar
  direccion text
  telefono varchar
  logo varchar
  logo_auto varchar
  divisa varchar
  correo_electronico varchar
  pagina_web varchar
  created_at timestamp 
  updated_at timestamp 
}
table usuarios {
  id int [pk, increment]
  name varchar
  email varchar 
  password varchar
  remember_token varchar 

  nombres varchar
  apellidos varchar
  tipo_documento varchar
  nro_documento varchar
  telefono varchar
  fecha_nacimiento date
  genero varchar
  direccion text
  foto varchar
  contacto_nombre varchar
  contacto_telefono varchar
  contacto_parentesco varchar

  created_at timestamp 
  updated_at timestamp 
}
Table espacios{
  id int [pk, increment]
  numero varchar
  estado varchar

  created_at timestamp 
  updated_at timestamp
}

Table tarifas {
  id int [pk, increment]
  nombre varchar
  tipo varchar
  cantidad int
  costo decimal(10,2)
  minutos_de_gracia int
  
  created_at timestamp 
  updated_at timestamp
}
